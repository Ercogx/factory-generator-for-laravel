{
    "name": "ercogx/factory-generator-for-laravel",
    "description": "A library that automatically fills the definition() method in the factory based on DB columns",
    "keywords": [
        "laravel",
        "factory",
        "faker",
        "autogenerate"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ercogx\\FactoryGeneratorLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ercogx",
            "email": "ercogx@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ercogx\\FactoryGeneratorLaravel\\FactoryGeneratorServiceProvider"
            ]
        }
    }
}
